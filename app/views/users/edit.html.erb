<main class="Update-Main">
  <h2 class="Change">
    Change password
  </h2>
  <%= form_with url: user_path, method: :put, id: "edit_user" do |form| %>
    <%= form.password_field :password,
                            placeholder: "Password" %>
    <br>
    <%= form.password_field :current_password,
                            placeholder: "Current Password" %>
    <br>
    <%= form.submit "Update", class: "Change-Button" %>
  <% end %>
</main>